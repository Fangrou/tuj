<?xml version="1.0" encoding="UTF-8"?>
<suite name="Suite" parallel="false">
	<!-- esb -->
	<parameter name="esb.conf.zookeeperServer" value="127.0.0.1:2181"></parameter>
	<parameter name="esb.conf.serviceActivityMonitorServer"
		value="http://192.168.0.216:8899/sam-server-war" />
	<parameter name="esb.conf.serviceActivityMonitorServer.remote.available"
		value="http://192.168.0.216:8899/sam-server-war" />
	<parameter name="esb.conf.serviceActivityMonitorServer.remote.stop"
		value="http://192.168.0.test:8080/sam-server-war" />
	<parameter name="esb.monitor.porttype.exist" value="Service" />
	<parameter name="esb.monitor.porttype.notexist" value="testfilterPorttype" />
	<parameter name="esb.monitor.receivenewevents.consumer" value="jobFirstConsumer" />
	<parameter name="esb.monitor.karajar" value="" />
	<parameter name="esb.monitor.pagesize" value="30" />
	<parameter name="esb.monitor.generateNumForTest" value="5" />
	<parameter name="esb.monitor.generateNumForPageDisplay" value="30" />
		
	<test name="test_generateEventsTest">
		<classes>
			<class name="com.talend.cases.esb.TestEsbServiceActivityGenerateEvents" />
		     <methods>
	            <include name="testGenerateEventsTest"/>
	         </methods>
		</classes>
	</test>
	
	<test name="test_monitor_activity_data">
		<classes>
			<class name="com.talend.cases.esb.TestEsbServiceActivityColumns" />
			<class name="com.talend.cases.esb.TestEsbServiceActivityFilterData" />
			<class name="com.talend.cases.esb.TestEsbServicePayloadContentsDisplay" />
			<class name="com.talend.cases.esb.TestEsbServiceActivitySelectRows" />		
		</classes>
	</test> 
	
	<test name="test_generateEventsPageDisplay">
		<classes>
			<class name="com.talend.cases.esb.TestEsbServiceActivityGenerateEvents" />
		     <methods>
	            <include name="testGenerateEventsForPageDisplay"/>
	         </methods>
		</classes>
	</test>
	
	<test name="test_acitvity_events_pagedisplay">
		<classes>
			<class name="com.talend.cases.esb.TestEsbServiceActivityPageDisplay" />
		</classes>
	</test>
	
	<test name="test_TestSamServerNotRunning">
		<classes>
			<class name="com.talend.cases.esb.TestSamServerNotRunning" />
		</classes>
	</test> 
</suite>

