<suite name="Suite" parallel="false">
	
	<!-- command line parameters -->

		
	<!-- Log4j parameters -->
	<parameter name="log4j.conf.logsPath" value="org/talend/tac/fileFolder/" />
	<parameter name="log4j.conf.logsName" value="talendappender.log" />
	
	<!-- SMTP wrong parameters -->	
	<parameter name="smtp.conf.wrong.mailServerHost" value="smtp.wrong.163.com" />
	<parameter name="smtp.conf.wrong.mailServerPort" value="25.wrong" />
	<parameter name="smtp.conf.wrong.mailUserName" value="lindy512163.com.wrong" />
	<parameter name="smtp.conf.wrong.mailPassword" value="talend.wrong" />
	<parameter name="smtp.conf.wrong.serverRequireSSL" value="false.wrong" />
	
	<!-- Scheduler parameters -->
