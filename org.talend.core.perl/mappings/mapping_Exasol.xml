<?xml version="1.0"?>
<mapping>
	<dbms product="Exasol" id="Exasol_id" label="Mapping Exasol"
		default="true">
		<dbTypes>
			<dbType type="VARCHAR" default="true" defaultLength="200" ignoreLen="false" ignorePre="true"/>
			<dbType type="CHAR" defaultLength="20" ignoreLen="false" ignorePre="true"/>
			<dbType type="NUMBER" defaultLength="20" defaultPrecision="10" ignoreLen="false" ignorePre="false"/>
			<dbType type="INT" ignoreLen="true" ignorePre="true"/>
			<dbType type="INTEGER" ignoreLen="true" ignorePre="true"/>
			<dbType type="LONG" ignoreLen="true" ignorePre="true"/>
			<dbType type="FLOAT" ignoreLen="true" ignorePre="true"/>
			<dbType type="BINARY_FLOAT" ignoreLen="true" ignorePre="true"/>
			<dbType type="DOUBLE PRECISION" ignoreLen="true" ignorePre="true"/>
			<dbType type="BINARY_DOUBLE" ignoreLen="true" ignorePre="true"/>
			<dbType type="DATE" ignoreLen="true" ignorePre="true"/>
		</dbTypes>

		<language name="perl">
			<talendToDbTypes>
				<talendType type="int">	
					<dbType type="INT" default="true"/>
					<dbType type="INTEGER"/>
					<dbType type="LONG"/>
				</talendType>
				<talendType type="decimal">
					<dbType type="NUMBER"/>
					<dbType type="FLOAT"/>
					<dbType type="BINARY_FLOAT"/>
					<dbType type="DOUBLE PRECISION"/>
					<dbType type="BINARY_DOUBLE"/>							
				</talendType>
				<talendType type="boolean">					
				</talendType>
				<talendType type="string">
					<dbType type="VARCHAR"/>
					<dbType type="CHAR"/>
					<dbType type="NCHAR"/>
				</talendType>
				<talendType type="date">
					<dbType type="DATE" default="true" />	
				</talendType>
				<talendType type="datetime">
				</talendType>
			</talendToDbTypes>
			<dbToTalendTypes><!-- Adviced mappings  -->
				<dbType type="VARCHAR">
					<talendType type="string" default="true" />
				</dbType>
				<dbType type="CHAR">
					<talendType type="string" default="true" />
				</dbType>
				<dbType type="NUMBER">
					<talendType type="decimal" default="true" />
				</dbType>
				<dbType type="INT">
					<talendType type="int" default="true" />
				</dbType>
				<dbType type="INTEGER">
					<talendType type="int" default="true" />
				</dbType>
				<dbType type="LONG">
					<talendType type="int" default="true" />
				</dbType>
				<dbType type="FLOAT">
					<talendType type="decimal" default="true" />
				</dbType>
				<dbType type="BINARY_FLOAT">
					<talendType type="decimal" default="true" />
				</dbType>				
				<dbType type="DOUBLE PRECISION">
					<talendType type="decimal" default="true" />
				</dbType>
				<dbType type="BINARY_DOUBLE">
					<talendType type="decimal" default="true" />
				</dbType>
				<dbType type="DATE">
					<talendType type="date" default="true" />				
				</dbType>
			</dbToTalendTypes>
		</language>
	</dbms>

</mapping>
