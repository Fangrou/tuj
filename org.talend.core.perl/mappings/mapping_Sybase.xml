			<dbType type="NCHAR" defaultLength="100" ignorePre="true" />
			<dbType type="NVARCHAR" defaultLength="200" ignorePre="true" />
		</dbTypes>
		<language name="perl">
			<talendToDbTypes>
				<talendType type="int">	
