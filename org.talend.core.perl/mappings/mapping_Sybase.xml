					<dbType type="UNIQUEIDENTIFIER"/>			
				</talendType>
				<talendType type="datetime">
					<dbType type="DATETIME" default="true" />
					<dbType type="DATE" />	
					<dbType type="SMALLDATETIME"/>
					<dbType type="TIMESTAMP"/>
