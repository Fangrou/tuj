<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="Talend"
            id="batch.generate.job.screenshot"
            name="Generate job screenshot in batch"/>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="batch.generate.job.screenshot"
            contextId="talend.global"
            schemeId="org.talend.core.scheme"
            sequence="CTRL+F12"/>
   </extension>
   
   <extension
         point="org.talend.core.global_actions">
      <GlobalAction class="org.talend.internal.utilities.actions.BatchGenJobScreenshotAction"/>
   </extension>
   
    <extension
         point="org.eclipse.ui.editors">
     	<editor
            class="org.talend.internal.utilities.actions.FakeMultiPageTalendEditor"
            contributorClass="org.talend.designer.core.ui.MultiPageEditorContributor"
            extensions="tl2"
            icon="icons/sample.gif"
            id="org.talend.internal.utilities.actions.FakeMultiPageTalendEditor"
            name="Designer">
      	</editor>
   </extension>
   
</plugin>
