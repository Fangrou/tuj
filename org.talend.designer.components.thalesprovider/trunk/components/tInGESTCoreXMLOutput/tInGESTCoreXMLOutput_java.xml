<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
		STARTABLE="false">
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>File/InGEST</FAMILY>
  </FAMILIES>
  
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

<!-- 
		<PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="1"
			REQUIRED="true">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.xml"</DEFAULT>
		</PARAMETER>
 -->
 
 		<PARAMETER NAME="MESSAGEKEY" FIELD="TEXT" NUM_ROW="1"
			REQUIRED="true">
			<DEFAULT>"myMessageKey"</DEFAULT>
		</PARAMETER>  		
		
		<PARAMETER NAME="ROW_TAG" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="3">
			<DEFAULT>"row"</DEFAULT>
		</PARAMETER>		

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="13">
			<DEFAULT />
		</PARAMETER>
	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>
<!-- 	
		<PARAMETER NAME="SPLIT" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="2">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="SPLIT_EVERY" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="2" SHOW_IF="SPLIT == 'true'">
			<DEFAULT>1000</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CREATE" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="2">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
-->
		<PARAMETER NAME="ROOT_TAGS" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="3" NB_LINES="2">
			<ITEMS>
				<ITEM NAME="TAG" />
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER NAME="MAPPING" FIELD="TABLE" REQUIRED="true"
			NUM_ROW="5" NB_LINES="5">
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="AS_ATTRIBUTE" FIELD="CHECK" />
				<ITEM NAME="SCHEMA_COLUMN_NAME" FIELD="CHECK"
					VALUE="true" />
				<ITEM NAME="LABEL" VALUE="label" />
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER NAME="USE_DYNAMIC_GROUPING" FIELD="CHECK"
			REQUIRED="true" NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="GROUP_BY" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="11" NB_LINES="3" SHOW_IF="USE_DYNAMIC_GROUPING == 'true'">
			<ITEMS>
				<ITEM NAME="COLUMN" FIELD="COLUMN_LIST" />
				<ITEM NAME="LABEL" VALUE="name" />
			</ITEMS>
		</PARAMETER>
<!-- 
		<PARAMETER NAME="FLUSHONROW" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="12">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FLUSHONROW_NUM" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="12" SHOW_IF="FLUSHONROW == 'true'">
			<DEFAULT>1</DEFAULT>
		</PARAMETER>
-->
		<PARAMETER NAME="ADVANCED_SEPARATOR" FIELD="CHECK" REQUIRED="true" NUM_ROW="13" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="THOUSANDS_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="14"
			SHOW_IF="(ADVANCED_SEPARATOR == 'true')">
			<DEFAULT>","</DEFAULT>
		</PARAMETER>		
		
		<PARAMETER NAME="DECIMAL_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="15"
			SHOW_IF="(ADVANCED_SEPARATOR == 'true')">
			<DEFAULT>"."</DEFAULT>
		</PARAMETER>
<!-- 
		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="16"
			REQUIRED="true">
			<DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>
		
	  	<PARAMETER NAME="DELETE_EMPTYFILE" FIELD="CHECK" REQUIRED="true" NUM_ROW="70">
	      <DEFAULT>false</DEFAULT>
	    </PARAMETER>
-->	    		
	</ADVANCED_PARAMETERS>

	<CODEGENERATION />

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>