			<DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>
		
	  	<PARAMETER NAME="DELETE_EMPTYFILE" FIELD="CHECK" REQUIRED="true" NUM_ROW="80" SHOW_IF="APPEND == 'false'">
